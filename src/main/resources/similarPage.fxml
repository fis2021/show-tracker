<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import java.net.URL?>
<?import javafx.scene.control.ScrollPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="fis.project.st.controllers.SimilarPageController"
            prefHeight="720.0" prefWidth="1280.0">
    <children>
        <fx:include source="navBar.fxml" />
        <AnchorPane layoutX="-7.0" layoutY="78.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <children>
                <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" layoutX="7.0" layoutY="2.0" prefHeight="627.2" prefWidth="1280.0">
                    <content>
                        <GridPane fx:id="similarGrid" prefHeight="613.0" prefWidth="1280.0">
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <columnConstraints>
                                <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
                            </columnConstraints>
                        </GridPane>
                    </content>
                </ScrollPane>
                <Text fx:id="errorMsg" layoutX="499.0" layoutY="144.0" styleClass="search-error" textAlignment="CENTER" />
            </children>
        </AnchorPane>
    </children>
    <stylesheets>
        <URL value="@homepage.css" />
        <URL value="@showPage.css" />
    </stylesheets>

</AnchorPane>
